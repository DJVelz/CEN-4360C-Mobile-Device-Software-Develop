let todoList = [];

let maxItems = 10;

while (todoList.length < maxItems) {
    let item = prompt("Enter a ToDo item:")

    if (todoList.includes(item)){
        alert("That item is already in the list. Please enter something different.")
    } else {
        todoList.push(item);
    }
    console.log("Your ToDo List: ", todoList);
}